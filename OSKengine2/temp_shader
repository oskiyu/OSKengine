Texture2D<float4> stexture : register(t0, space0);
SamplerState _stexture_sampler : register(s0, space0);

static float4 outColor;
static float4 color;
static float2 texCoords;

struct SPIRV_Cross_Input
{
    float4 color : TEXCOORD0;
    float2 texCoords : TEXCOORD1;
};

struct SPIRV_Cross_Output
{
    float4 outColor : SV_Target0;
};

void frag_main()
{
    outColor = float4(color * stexture.Sample(_stexture_sampler, texCoords));
}

SPIRV_Cross_Output main(SPIRV_Cross_Input stage_input)
{
    color = stage_input.color;
    texCoords = stage_input.texCoords;
    frag_main();
    SPIRV_Cross_Output stage_output;
    stage_output.outColor = outColor;
    return stage_output;
}

